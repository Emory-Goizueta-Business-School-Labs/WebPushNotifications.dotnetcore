    <UserSecretsId>93177d1f-e6a8-47fa-b37f-f140e6035520</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="7.0.10" />
